{
  "name": "wabpack-setup-for-react",
  "description": "Mininal Webpack and React.js Starter boilerplate for TypeScript projects.",
  "version": "1.0.1",
  "author": "Mateus V. Farias",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fariasmateuss/webpack-setup-for-react.git"
  },
  "scripts": {
    "dev": "webpack serve",
    "build": "cross-env NODE_ENV=production webpack",
    "predeploy": "cross-env NODE_ENV=production webpack",
    "deploy": "gh-pages -d build",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "cy:open-only": "cypress open",
    "cy:run-only": "cypress run",
    "cy:open": "start-server-and-test dev 3000 cy:open-only",
    "cy:run": "start-server-and-test dev 3000 cy:run-only",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@testing-library/react": "^13.0.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-router-dom": "6.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.9",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.5",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.3",
    "@types/jest": "^27.4.1",
    "@types/react-dom": "17.0.14",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "babel-loader": "8.2.4",
    "babel-plugin-module-resolver": "4.1.0",
    "commitizen": "4.2.4",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "cypress": "^9.5.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.12.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
    "gh-pages": "3.2.3",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jsdom": "^19.0.0",
    "lint-staged": "12.3.7",
    "node-sass": "7.0.1",
    "prettier": "2.6.2",
    "process": "0.11.10",
    "react-refresh": "0.11.0",
    "sass-loader": "12.6.0",
    "semantic-release": "^19.0.2",
    "start-server-and-test": "^1.14.0",
    "style-loader": "3.3.1",
    "ts-jest": "^27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.3",
    "webpack": "5.71.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.8.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
